{
  "name": "rag-backend",
  "version": "1.0.0",
  "description": "RAG Backend for text processing and embedding generation",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "setup": "node setup.js",
    "check-env": "node -e \"console.log('Environment check:', process.env.SUPABASE_URL ? '✅' : '❌', 'SUPABASE_URL')\"",
    "examples": "node examples/retrieval_examples.js",
    "examples:answers": "node examples/answer_examples.js",
    "examples:reranking": "node examples/reranking_examples.js",
    "test:system": "node scripts/test-system.js",
    "verify:deployment": "node scripts/verify-deployment.js",
    "deploy": "bash scripts/deploy-production.sh",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deployment/windows-deploy.ps1",
    "deploy:windows:simple": "scripts/deploy-windows.cmd",
    "docker:build": "docker build -t rag-backend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env rag-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "@supabase/supabase-js": "^2.38.4",
    "openai": "^4.20.1",
    "cohere-ai": "^7.10.0",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  }
}